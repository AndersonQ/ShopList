<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" 
    package="br.eti.andersonq.shoplist">
    
    <uses-sdk android:targetSdkVersion="17" 
        android:minSdkVersion="10"/>
    
    <application 	android:icon="@drawable/icon"
    				android:theme="@android:style/Theme.Holo.Light">
        
        <activity android:name="br.eti.andersonq.ItemsMain" 
            android:label="@string/app_name">
                
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
       
        </activity>
       
        <activity android:name="br.eti.andersonq.ItemEdit" 
            android:label="@string/app_name"
            android:parentActivityName="br.eti.andersonq.ItemsMain"/>
            
        <activity android:name="br.eti.andersonq.ListsEdit" 
            android:label="@string/app_name"
            android:parentActivityName="br.eti.andersonq.ListsMain"/>
        <activity android:name="br.eti.andersonq.ListsMain"
		android:label="@string/list_choose"
            	android:parentActivityName="br.eti.andersonq.ListsMain"/>
   
	</application>
</manifest> 
